{
    "name": "axl-media-org/sportradar-sdk",
    "description": "PHP-based SDK to work with the Sportradar API & live events.",
    "keywords": ["laravel", "php", "sportradar", "sdk", "sports"],
    "license": "Apache-2.0",
    "homepage": "https://github.com/axl-media-org/sportradar-sdk",
    "authors": [
        {
            "name": "AXL Media",
            "homepage": "https://axl.media",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^6.5|^7.0"
    },
    "autoload": {
        "psr-4": {
            "AxlMedia\\SportradarSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AxlMedia\\SportradarSdk\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^5.0|^6.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "AxlMedia\\SportradarSdk\\SportradarSdkServiceProvider"
            ]
        }
    }
}
